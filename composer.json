{
  "name": "afzalroq/yii2-cms",
  "description": "Yii2 cms extension",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "content",
    "cms"
  ],
  "homepage": "https://www.github.com/afzalroq/yii2-cms",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Proactive.uz",
      "email": "info@proactive.uz",
      "homepage": "https://proactive.uz"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0",
    "zelenin/yii2-slug-behavior": "~1.5.1",
    "kartik-v/yii2-widget-fileinput": "@dev",
    "yii-dream-team/yii2-upload-behavior": "^1.2.3",
    "mihaildev/yii2-elfinder": "*",
    "sadovojav/yii2-ckeditor": "dev-master",
    "gregwar/image": "2.*",
    "kartik-v/yii2-datecontrol": "dev-master",
    "kartik-v/yii2-widget-datepicker": "^1.4",
    "kartik-v/yii2-widget-datetimepicker": "^1.4",
    "symfony/var-dumper": "^5.1.4",
    "richardfan1126/yii2-sortable-gridview": "*",
    "slatiusa/yii2-nestable": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "afzalroq-cms\\": ""
    }
  },
  "extra": {
    "bootstrap": "afzalroq\\cms\\Bootstrap"
  }
}
